<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>dashboard</servlet-name>
        <servlet-class>control.dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeDAO</servlet-name>
        <servlet-class>dal.EmployeeDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listUser</servlet-name>
        <servlet-class>control.listUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteUser</servlet-name>
        <servlet-class>control.deleteUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addUser</servlet-name>
        <servlet-class>control.addUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editUser</servlet-name>
        <servlet-class>control.editUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listRoom</servlet-name>
        <servlet-class>control.listRoom</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeDAO</servlet-name>
        <url-pattern>/EmployeeDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listUser</servlet-name>
        <url-pattern>/listUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteUser</servlet-name>
        <url-pattern>/deleteUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addUser</servlet-name>
        <url-pattern>/addUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editUser</servlet-name>
        <url-pattern>/editUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listRoom</servlet-name>
        <url-pattern>/listRoom</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>guestHomePage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
